CREATE TABLE `todoweb`.`persona` ( `id` INT NOT NULL , `alias` VARCHAR(25) NOT NULL , `nombre` VARCHAR(25) NOT NULL , `apellido` VARCHAR(25) NOT NULL , `email` VARCHAR(25) NOT NULL , PRIMARY KEY (`id`)) ENGINE = InnoDB; 
